          -moz-transition: opacity 0.1s ease-in-out;
          -o-transition: opacity 0.1s ease-in-out;
          transition: opacity 0.1s ease-in-out;
          border-radius: 0px;
        }

        /*--- FX ---*/

        @keyframes fade {
          from { opacity: 1.0; }
          50% { opacity: 0.4; }
          to { opacity: 1.0; }
        }

        @-webkit-keyframes fade {
          from { opacity: 1.0; }
